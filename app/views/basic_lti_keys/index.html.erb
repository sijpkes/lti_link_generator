<p id="notice"><%= notice %></p>

<h1>Listing Basic Lti Keys</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @basic_lti_keys.each do |basic_lti_key| %>
      <tr>
        <td><%= basic_lti_key.title %></td>
        <td><%= basic_lti_key.body %></td>
        <td><%= link_to 'Show', basic_lti_key %></td>
        <td><%= link_to 'Edit', edit_basic_lti_key_path(basic_lti_key) %></td>
        <td><%= link_to 'Destroy', basic_lti_key, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Basic lti key', new_basic_lti_key_path %>
